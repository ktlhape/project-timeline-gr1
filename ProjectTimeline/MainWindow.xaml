<Window x:Class="ProjectTimeline.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectTimeline"
        mc:Ignorable="d"
        Title="MainWindow" Height="550" Width="1100">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="201*"/>
            <ColumnDefinition Width="599*"/>
        </Grid.ColumnDefinitions>
        <StackPanel Background="Blue">
            <Button x:Name="btnCapture" Content="Capture" FontSize="25" Height="50" Margin="0 25" Click="btnCapture_Click"/>
            <Button x:Name="btnDisplay" Content="Display" FontSize="25" Height="50" Margin="0 25" Click="btnDisplay_Click" />
            <Button x:Name="btnFilter" Content="Filter" FontSize="25" Height="50" Margin="0 25" Click="btnFilter_Click" />

        </StackPanel>
        <Frame Grid.Column="1" Name="frmContainer" NavigationUIVisibility="Hidden" Padding="30"/>

    </Grid>
</Window>
